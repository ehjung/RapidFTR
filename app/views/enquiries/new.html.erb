<h1><%= link_to t("enquiries.label"), enquiries_path %> &gt; <%= t("enquiries.register_new_enquiry") %></h1>

<%= form_tag enquiries_path, :multipart => true do %>
  <div class="page-content-new">
    <%= error_messages_for :enquiry %>
    <%= hidden_field_tag :is_nil, true %>
    </br>

    <div class="form_info">
        <span class="tool-tip-label"><%= label_tag t("enquiries.enquirer_name"), nil, class: 'key' %></span>
        <input type="text" id="enquiry_enquirer_name" name="enquiry[enquirer_name]" />  
    </div>

    </br>
    <div class="side-tab">
      <%= render :partial => "tabs", :object => @form_sections %>
    </div>
    <div class="side-tab-content">
      <div class="clearfix"></div>
      <%= render :partial => "form_section", :collection => @form_sections %>
      <div class="btn_panel">  
          <%= discard_button :back %>
          <%= submit_button %>    
      </div> 
    </div>    
  </div>
  <div class="clearfix"></div>
<% end %>


